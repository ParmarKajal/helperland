
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title></title>
    <!-- Latest compiled and minified CSS -->
    <!--<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">-->
    <!-- Latest compiled JavaScript -->
    <!--<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>-->

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css"integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.2/css/bootstrap.css">   
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.11.3/css/dataTables.bootstrap5.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/responsive/2.2.9/css/responsive.bootstrap5.min.css">
    <link href="~/css/CustomerDashboard.css" rel="stylesheet" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@500&display=swap" rel="stylesheet">
</head>

<body>

    <!--navigation menu-->
    <header>
        <nav class="navbar navbar-expand-lg  navbar-dark main-nav">
            <div class="container-fluid" id="navbar">
                <a class="navbar-brand" href="#"><img src="../image/logo-small.png"></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#collapsibleNavbar">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse justify-content-end" id="collapsibleNavbar">
                    <ul class="navbar-nav">
                        <li class="nav-item px-1">
                            <a class="nav-link text-center rounded-pill menu-simple border border-1 px-3 me-2" style="background-color:#29626d;" href="#">
                                Book
                                Now
                            </a>
                        </li>
                        <li class="nav-item px-1">
                            <a class="nav-link  me-2 hovermenu" href="#">Prices & services</a>
                        </li>
                        <li class="nav-item px-1">
                            <a class="nav-link me-2 hovermenu" href="#">Warranty</a>
                        </li>
                        <li class="nav-item px-1">
                            <a class="nav-link me-2 hovermenu" href="#">Blog</a>
                        </li>
                        <li class="nav-item px-1">
                            <a class="nav-link me-4 hovermenu" href="contact.html" target="_blank">Contact</a>
                        </li>
                    </ul>
                </div>

                <div class="vertical-line">
                    <div class="nav-item">
                        <a class="nav-link notification" href="#">
                            <img src="../image/icon-notification.png" style="position: absolute;">
                            <div class="notification-num">
                                <sup>2</sup>
                            </div>
                        </a>
                    </div>
                </div>

                <div class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown">
                        <img src="../image/admin-user.png">
                    </a>
                    <ul class="dropdown-menu">
                        <li><a class="dropdown-item" href="#">Invoices</a></li>
                        <li><a class="dropdown-item" asp-action="Profile" asp-controller="Customer">My Settings</a></li>
                        <li><a class="dropdown-item" href="#">Logout</a></li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <!--user name-->
    <section class="title">
        <h1 class="welcome-text text-center">Welcome,<b> @ViewBag.spname!</b></h1>
        <div class="welcome-line"></div>
    </section>

    <!--main section-->

    <main class="px-0">
        <div class="container-fluid px-0">
            <div class="row px-0">

                <!------------------------table 1---------------------------------------------->
                <div class="col-sm-3 customer-dashboard-table1  flex-column">
                    <nav class="navbar">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link customer-dashboard-table1-content active px-3">Dashboard</a>
                            </li>
                            <hr class="line">
                            <li class="nav-item">
                                <a class="nav-link customer-dashboard-table1-content  px-3" href="#New-Service-Request-table" onclick="spnewservicerequest()">New Service Request</a>
                            </li>
                            <hr class="line">
                            <li class="nav-item">
                                <a class="nav-link customer-dashboard-table1-content px-3" href="#Upcoming-Service-Provider-table" onclick="spupcomingservice()">Upcoming Services</a>
                            </li>
                            <hr class="line">
                            <li class="nav-item">
                                <a class="nav-link customer-dashboard-table1-content px-3">Service Schedule</a>
                            </li>
                            <hr class="line">
                            <li class="nav-item">
                                <a class="nav-link customer-dashboard-table1-content px-3" href="#Upcoming-Service-Provider-history-table" onclick="spServiceHistory()">Service History</a>
                            </li>
                            <hr class="line">
                            <li class="nav-item">
                                <a class="nav-link customer-dashboard-table1-content px-3" href="#My-Rating-Table" onclick="spmyrating()">My Ratings</a>
                            </li>
                            <hr class="line">
                            <li class="nav-item">
                                <a class="nav-link customer-dashboard-table1-content px-3" href="#Block-Customer" onclick="spBlockCustomer()">Block Customer</a>
                            </li>
                            <hr class="line">
                        </ul>
                    </nav>
                </div>

                <!-----------------------table 2---------------------------->

                <div class="col-sm-9 px-0">
                    <div id="Upcoming-Service-Provider-table">
                        @await Html.PartialAsync("SPUpcomingService.cshtml")
                    </div>

                    <div id="Upcoming-Service-Provider-history-table" style="display:none;">
                        @await Html.PartialAsync("SPServiceHistory.cshtml")
                    </div>

                    <div id="New-Service-Request-table">

                    </div>


                    <div id="Block-Customer">

                    </div>

                    <div id="My-Rating-Table">

                    </div>

                </div>
            </div>
        </div>
    </main>

    <footer>

        <div class="container d-flex justify-content-between align-items-center  py-3">
            <div>
                <img src="../image/footer-logo.png" alt="" width="100%">
            </div>
            <div class="d-flex flex-wrap text-center justify-content-center">
                <a class="px-3 pb-3 mt-4"
                   asp-controller="Home" asp-action="Index" target="_blank">HOME</a>
                <a class="px-3 pb-3 mt-4"
                   asp-controller="Home" asp-action="About" target="_blank">ABOUT</a>
                <a class="px-3 pb-3 mt-4"
                   href="#">TESTIMONIALS</a>
                <a class="px-3 pb-3 mt-4"
                   asp-controller="Home" asp-action="FAQ" target="_blank">FAQS</a>
                <a class="px-3 pb-3 mt-4"
                   href="#">INSURRANCE POLICY</a>
                <a class="px-3 pb-3 mt-4"
                   href="#">IMPRESSUM</a>
            </div>

            <div>
                <a style="color: white; text-decoration: none;" href="#" class="fab fa-facebook-f"></a>
                <a style="color: white; text-decoration: none;" href="#" class="fab fa-instagram"></a>
            </div>
        </div>


        <div class="container mb-0" style="font-family: 'Roboto';">
            <div class="footer-line" style="width:1120px; height:1px; padding-left: 120px;  background-color: #646464;"></div>
            <div style="color: #9BA0A3; font-size: 14px;" class="text-center mt-4 mb-0 py-2">
                ©2018 Helperland. All
                rights
                reserved. &nbsp &nbsp Terms and Conditions | Privacy Policy
            </div>
        </div>
    </footer>



    <script>
    function spupcomingservice() {

        $("#Upcoming-Service-Provider-history-table").hide();


        $("#Upcoming-Service-Provider-table").show();

        $("#New-Service-Request-table").hide();
        $("#Block-Customer").hide();
        $("#My-Rating-Table").hide();

    }
    function spServiceHistory() {


    ;


        $("#Upcoming-Service-Provider-history-table").show();


        $("#Upcoming-Service-Provider-table").hide();
        $("#New-Service-Request-table").hide();

        $("#Block-Customer").hide();
        $("#My-Rating-Table").hide();

        }


        function spnewservicerequest() {

            $("#Upcoming-Service-Provider-table").hide();
            $("#Upcoming-Service-Provider-history-table").hide();
            $("#Block-Customer").hide();
            $("#My-Rating-Table").hide();
            $("#New-Service-Request-table").show();
            $("#New-Service-Request-table").load('@Url.Action("SPNewServiceRequest","ServiceProvider")');




        }

         function spBlockCustomer() {
           // $("#New-Service-Request-table").show();
            $("#Upcoming-Service-Provider-table").hide();
            $("#Upcoming-Service-Provider-history-table").hide();
             $("#New-Service-Request-table").hide();
             $("#My-Rating-Table").hide();
             $("#Block-Customer").show();
            $("#Block-Customer").load('@Url.Action("SPBlockCustomer","ServiceProvider")');



        }

        function spmyrating() {
             $("#Upcoming-Service-Provider-table").hide();
            $("#Upcoming-Service-Provider-history-table").hide();
            $("#New-Service-Request-table").hide();
            $("#Block-Customer").hide();
            $("#My-Rating-Table").show();
            $("#My-Rating-Table").load('@Url.Action("SPMyRating","ServiceProvider")');

        }

    </script>

